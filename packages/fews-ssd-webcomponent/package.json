{
  "name": "@deltares/fews-ssd-webcomponent",
  "version": "2.0.0-rc.4",
  "description": "Web component for FEWS Schematic Status Display",
  "author": {
    "name": "Werner Kramer",
    "email": "werner.kramer@deltares.nl"
  },
  "keywords": [
    "FEWS",
    "Schematic Status Display web component"
  ],
  "license": "MIT",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/fews-ssd-webcomponent/fews-ssd-webcomponent.esm.js",
  "exports": {
    ".": {
      "import": "./dist/fews-ssd-webcomponent/fews-ssd-webcomponent.esm.js",
      "require": "./dist/fews-ssd-webcomponent/fews-ssd-webcomponent.cjs.js"
    },
    "./schematic-status-display": {
      "import": "./dist/components/schematic-status-display.js",
      "types": "./dist/components/schematic-status-display.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "private": false,
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "dependencies": {
    "@deltares/fews-pi-requests": "^2.0.0-beta.1",
    "@deltares/fews-ssd-requests": "^2.0.0-rc.3",
    "@stencil/core": "^4.38.3"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "jest": "^30.2.0",
    "jest-cli": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-junit": "^16.0.0",
    "jest-teamcity-reporter": "^0.9.0",
    "puppeteer": "^24.29.1",
    "typescript": "^5.9.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Deltares/fews-ssd-requests.git",
    "directory": "packages/fews-ssd-webcomponent"
  },
  "bugs": {
    "url": "https://github.com/Deltares/fews-ssd-requests/issues"
  },
  "publishConfig": {
    "access": "public",
    "tag": "rc",
    "registry": "https://registry.npmjs.org"
  }
}
