{
  "name": "@deltares/fews-pi-requests",
  "version": "0.5.3",
  "description": "template for fews typescript libraries",
  "author": {
    "name": "Werner Kramer",
    "email": "werner.kramer@deltares.nl"
  },
  "keywords": [
    "FEWS",
    "TYPESCRIPT"
  ],
  "license": "MIT",
  "main": "./dist/fews-pi-requests.umd.js",
  "typings": "./dist/types/index.d.ts",
  "private": false,
  "scripts": {
    "build": "npm run clean && tsc && webpack",
    "clean": "shx rm -rf dist",
    "doc": "typedoc --out doc src",
    "lint": "eslint src/**.ts",
    "lint:fix": "eslint src/**.ts --fix",
    "test:unit": "jest unit --collect-coverage",
    "test:e2d": "jest e2e",
    "test": "jest --coverage"
  },
  "dependencies": {
    "@types/jsdom": "^16.2.14",
    "axios": "^0.26.1",
    "dotenv": "^16.0.0",
    "jsdom": "^19.0.0"
  },
  "devDependencies": {
    "@deltares/fews-pi-requests": "^0.5.3",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "eslint": "^6.8.0",
    "fetch-mock": "^9.11.0",
    "fetch-mock-jest": "^1.5.1",
    "jest": "^27.3.1",
    "jest-junit": "^13.0.0",
    "jest-teamcity-reporter": "^0.9.0",
    "node-fetch": "^3.2.0",
    "shx": "^0.3.2",
    "ts-jest": "^27.0.7",
    "ts-loader": "^6.2.1",
    "typedoc": "^0.22.6",
    "typescript": "^4.4.0",
    "webpack": "^5.59.0",
    "webpack-cli": "^4.9.1"
  },
  "jest-junit": {
    "outputDirectory": "reports/",
    "outputName": "junit.xml"
  },
  "engines": {
    "node": ">=0.14"
  },
  "files": [
    "dist/*",
    "src/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Deltares/fews-pi-requests"
  },
  "bugs": {
    "url": "https://github.com/Deltares/fews-pi-requests/issues"
  }
